<?xml version="1.0" encoding="UTF-8"?><database>
    <element audit="true" extends="task" label="Acreditació" max_length="40" name="x_udg_q_acreditacio" sizeclass="3" type="collection">
        <element label="Aprovació autoinforme CAI" max_length="40" name="aprovacio_autoinforme_cai" type="glide_date"/>
        <element label="Aprovació autoinforme CQ Centre" max_length="40" name="aprovacio_autoinforme_cq_centre" type="glide_date"/>
        <element label="Aprovació autoinforme Junta Centre" max_length="40" name="aprovacio_autoinforme_junta_centre" type="glide_date"/>
        <element label="Aprovació CQ UdG" max_length="40" name="aprovacio_cq_udg" type="glide_date"/>
        <element label="Aprovació informe CAI definitiu" max_length="40" name="aprovacio_informe_cai_definitiu" type="boolean"/>
        <element label="DMA" max_length="40" name="dma" type="glide_date"/>
        <element label="Grup de treball" max_length="1024" name="grup_de_treball" type="url"/>
        <element label="Inici procés" max_length="40" name="inici_proces" type="glide_date"/>
        <element label="Inici procés definitiu" max_length="40" name="inici_proces_definitiu" type="boolean"/>
        <element choice="1" label="Resultat" max_length="40" name="resultat" type="integer">
            <choice>
                <element inactive_on_update="false" label="No acreditat" sequence="1" value="1"/>
                <element inactive_on_update="false" label="Acreditat amb condicions" sequence="2" value="2"/>
                <element inactive_on_update="false" label="Acreditat" sequence="3" value="3"/>
                <element inactive_on_update="false" label="Acreditat amb excel·lència" sequence="4" value="4"/>
            </choice>
        </element>
        <element label="SGQ" mandatory="true" max_length="32" name="sgq" reference="x_udg_q_sgq" type="reference"/>
        <element label="Sincronitzada" max_length="32" name="sincronitzada" reference="x_udg_q_acreditacio" reference_qual="javascript:&quot;sgq=&quot;+current.sgq" type="reference" use_reference_qualifier="advanced"/>
        <element dependent="sgq" dependent_on_field="sgq" label="Titulació" mandatory="true" max_length="32" name="titulacio" reference="x_udg_q_titulacio" type="reference" use_dependent_field="true"/>
        <element label="Visita CAE" max_length="40" name="visita_cae" type="glide_date"/>
        <element label="Visita CAE (Max)" max_length="40" name="visita_cae_max" type="glide_date"/>
    </element>
</database>
