<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Fita" max_length="40" name="x_udg_q_fita" type="collection">
        <element label="Data" mandatory="true" max_length="40" name="data" type="glide_date_time"/>
        <element display="true" label="Name" max_length="100" name="nom" type="string"/>
        <element label="SGQ" mandatory="true" max_length="32" name="sgq" reference="x_udg_q_sgq" type="reference"/>
        <element choice="1" label="Tipus" max_length="40" name="tipus" type="string">
            <choice>
                <element label="Acreditació" value="Acreditació"/>
                <element hint="Data Màxima d'Acreditació" label="DMA" value="DMA"/>
                <element label="Verificació" value="Verificació"/>
                <element hint="Visita del Comité d'Avaluació AQU" label="Visita Comité" value="Visita comité"/>
            </choice>
        </element>
        <element choice="1" dependent="sgq" dependent_on_field="sgq" label="Titulació" max_length="32" name="titulacio" reference="x_udg_q_titulacio" type="reference" use_dependent_field="true">
            <choice/>
        </element>
        <index name="index">
            <element name="titulacio"/>
        </index>
        <index name="index2">
            <element name="sgq"/>
        </index>
    </element>
</database>
