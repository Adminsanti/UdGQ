        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
	
	if (current.state == -5) current.assigned_to=gs.getUserID(); //esborranys s'assignen a qui desa

    if (current.state == 2) { //Només per objectius en curs
        if (current.assigned_to == '' || current.responsable.members.toString().indexOf(current.assigned_to) == -1) //L'assignat està en blanc o ja no pertany a la funció
            if (current.responsable != '' && current.responsable.usuari != '') {
                current.assigned_to = current.responsable.usuari;
                gs.addInfoMessage(current.number + " s'ha assignat a " + current.assigned_to.name);
                new log("Assignat al resposable per la BR Objectiu " + current.number + " assignat a " + current.assigned_to.name);
            } else new log(current.number + " Error d'assignació a la BR Objectiu assignar al responsable", 'error');
    }
})(current, previous);]]></script>
