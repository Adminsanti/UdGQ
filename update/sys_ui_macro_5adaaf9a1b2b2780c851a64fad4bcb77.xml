        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
<g:evaluate var="jvar_grSe" object="true">
    var grSe = new GlideRecord("x_udg_q_subestandard");
	grSe.orderBy('nom')
    grSe.query();
    grSe;
</g:evaluate>

<TABLE class="blueTable" >   <!-- style="width:1000px" -->
<j:while test="${jvar_grSe.next()}">
	
	<!--
	<TR>
		<TD COLSPAN="5">
			${grSe.nom}
		</TD>
	</TR>
-->	
	<g:evaluate var="jvar_grOb" object="true">
    var grOb = new GlideRecord("x_udg_q_objectiumillora");
	grOb.addEncodedQuery('state=2^NQstate=3^closed_at>=javascript:gs.beginningOfLastYear()^tancament!=-5'); 
	grOb.addQuery('subestandard',grSe.sys_id);
    grOb.query();
    grOb;
	</g:evaluate>
	<j:while test="${jvar_grOb.next()}">
		<TR>
			<TD>
				 <B>${jvar_grOb.getValue('number')}</B> 
				<BR>
					${jvar_grOb.getValue('short_description')} 
				</BR>
			</TD>
			<TD>
				${grOb.state.getDisplayValue()}
			</TD>
			<TD>
				<j:if test="${grOb.state==3}">${grOb.tancament.getDisplayValue()}
				</j:if>
				<j:if test="${grOb.state==2}">${grOb.assoliment.getDisplayValue()}% 			
					<g:evaluate var="jvar_Dat" object="true">
			         Dat = new GlideDate();
					 Dat.setValue(grOb.due_date);
					 var Caducat = Dat.onOrBefore(new GlideDate());
					 Dat
					</g:evaluate>
					<BR>
					<j:if test="${Caducat}">
						<FONT style="color: red;">
						${Dat.getDisplayValue()}
						</FONT>
					</j:if>
					<j:if test="${!Caducat}">
						${Dat.getDisplayValue()}
					</j:if>
					</BR>
				</j:if>
			</TD>
		 			
			<j:choose>
			   <j:when test="${grOb.priority==2}"><TD style="background-color:tomato;">${grOb.priority.getDisplayValue()}</TD></j:when>
			   <j:when test="${grOb.priority==3}"><TD>${grOb.priority.getDisplayValue()}</TD></j:when>
			   <j:otherwise><TD>${grOb.priority.getDisplayValue()}</TD></j:otherwise>
			</j:choose>
	
			<TD>${grOb.recomanacions.getDisplayValue()}</TD>
		</TR>
		</j:while>
	</j:while>
</TABLE>
</j:jelly>]]></xml>
    </sys_ui_macro>
</record_update>
