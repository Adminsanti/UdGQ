<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>x_udg_q_informeseguiment.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

<style>
	<g2:macro_invoke macro="x_udg_q_blueTable" />	
</style>
	
	<H2>$[gs.getMessage("Informe de Seguiment")]</H2> 
	<p>$[sysparm_id]</p>
	
<g:evaluate var="jvar_grS" object="true">
    var grS = new GlideRecord("x_udg_q_subestandard");
	grS.orderBy('nom')
    grS.query();
    grS;
</g:evaluate>

<j:while test="${jvar_grS.next()}">
	<H3>
	${grS.nom}
	</H3>
	<BR></BR>
	<g:evaluate var="jvar_grO" object="true">
    var grO = new GlideRecord("x_udg_q_objectiumillora");
	var qpar=RP.getParameterValue("sysparm_id");
	if (qpar!="") grO.addEncodedQuery(RP.getParameterValue("sysparm_id"));
	grO.addEncodedQuery('state!=-5'); //No esborranys
	grO.addEncodedQuery('tancament!=-5^state=3'); //No tancats esborrat
	grO.addEncodedQuery("state=2^ORstate=3^closed_at>=javascript:gs.beginningOfOneYearAgo()");	
    grO.query();
    grO;
	</g:evaluate>

	<j:while test="${jvar_grO.next()}">
			<g2:macro_invoke macro="x_udg_q_taulaObjectiu" number="${jvar_grO.getValue('number')}" short="${jvar_grO.getValue('short_description')}" sysid="${grO.sys_id}" estat="${grO.state.getDisplayValue()}" assoliment="${grO.assoliment}" valoracio="${grO.valoracio}" tipustancament="${grO.tancament.getDisplayValue()}" datatancament="${grO.closed_at.getDisplayValue()}" titulacions="${grO.implica_titulacions.getDisplayValue()}" recomanacions="${grO.recomanacions.getDisplayValue()}" recomanacioobl="${grO.recomanacioobligatoria}" sgq="${grO.sgq.getDisplayValue()}"/>	
			<BR></BR>
	</j:while>
</j:while>
</j:jelly>]]></html>
