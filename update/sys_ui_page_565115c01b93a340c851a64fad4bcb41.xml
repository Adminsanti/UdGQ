<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>x_udg_q_plademillora.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	
	<g2:evaluate jelly="true">
		var hola="FC";
	</g2:evaluate>

    <p>$[gs.getMessage("Pla de Millora")] $[hola]
    </p>
	
	<g:evaluate var="jvar_grO" object="true">
    var grO = new GlideRecord("x_udg_q_objectiumillora");
    grO.query();
    grO;
	</g:evaluate>
	
	<ul>
		<j:while test="${jvar_grO.next()}">
			<li>${jvar_grO.getValue('number')} - ${jvar_grO.getValue('short_description')} </li>
			<g:evaluate var="jvar_grM2M" object="true">
			var grM2M = new GlideRecord("x_udg_q_m2m_accions_objectius");
			grM2M.addEncodedQuery("objectiu_millora="+grO.sys_id);
			grM2M.query();
			grM2M;
			</g:evaluate>
			<ul>
			<j:while test="${jvar_grM2M.next()}">
				<g:evaluate var="jvar_grA" object="true">
				var grA = new GlideRecord("x_udg_q_acciomillora");
				grA.get(grM2M.accio_millora);
				grA;
				</g:evaluate>
				<li>${jvar_grA.getValue('number')} - ${jvar_grA.getValue('short_description')}
					<BR>Contribuci√≥ ${jvar_grM2M.getValue('percentatge')}%</BR>
					</li>
			</j:while>

			</ul>
		</j:while>
	</ul>	
	
	<H1>FI</H1>

</j:jelly>]]></html>
