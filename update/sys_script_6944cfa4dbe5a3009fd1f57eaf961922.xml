        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	//gs.addInfoMessage("Recomanacions1: "+current.recomanacions.split(',').length);
	var li = new ListUtil(current.recomanacions);
	if (previous.recomanacions != "") li.addListToList(previous.recomanacions);
	var arr=li.getList().split(',');
	for(var i = 0; i < arr.length; i++) {
		var grRec = new GlideRecord('x_udg_q_recomanacions');
		grRec.get('sys_id',arr[i]);
		var R = new x_udg_q.Reco();
		var NouEstat= R.getEstat(grRec);
		if (NouEstat != grRec.estat) {
			grRec.estat=NouEstat;
			grRec.update();
		}
	}
	
})(current, previous);]]></script>
