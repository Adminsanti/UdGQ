        <value>(function execute(inputs, outputs) {
	new log('Inici del procés automàtic d\'actualització de l\'estat de acreditacions (flow)');  
	var grCAI = new GlideRecord('x_udg_q_cai');
	grCAI.query();
	while (grCAI.next()) {
  		var nouestat= new CAI().getEstat(grCAI);
  		if (grCAI.state!= nouestat) {
   			grCAI.state= nouestat;
    		grCAI.update();
    		new log(grCAI.number+" actualitzat estat a "+grCAI.getDisplayValue('state'));
  		}
	}
	new log('Fi del procés automàtic d\'actualització de l\'estat de acreditacions (flow)'); 
