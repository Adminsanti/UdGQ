        <script><![CDATA[doDefineixAccioR();

function doDefineixAccioR() {
    if (current.responsable == '') {
        current.update(); //gravem igualment
        gs.addErrorMessage(current.number + " s'ha desat com esborrany però NO es pot incorporar al Pla de Millora per <B>manca de responsable</B>");
    } else {
        current.state = 2;
        current.declarat = new GlideDateTime().toString();
        current.declarat_per = gs.getUserID();
        if (current.valoracio != "") {
            current.valorat_per = gs.getUserID();
            current.valorat = new GlideDateTime().toString();
        }
		if (current.responsable.membres.toString().indexOf(current.assigned_to)==-1) 
			current.assigned_to=current.responsable.usuari;
        current.update();
        gs.addInfoMessage(current.number + " Incorporada al Pla de Millora");
        new AccioUtils().actualitzaExecucioTotsObjectius(current);
    }
    action.setRedirectURL(current); //Roman
}]]></script>
