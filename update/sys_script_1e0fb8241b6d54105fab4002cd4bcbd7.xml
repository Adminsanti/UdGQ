        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    
	var notifica_a = [];
    var grSgq = new GlideRecord('x_udg_q_sgq');
	grSgq.addQuery('actiu', true);
    grSgq.query();
    while (grSgq.next()) {
        if (notifica_a.indexOf(grSgq.responsable.usuari.toString())==-1) notifica_a.push(grSgq.responsable.usuari.toString());
		if (grSgq.suport && notifica_a.indexOf(grSgq.suport.usuari.toString())==-1) notifica_a.push(grSgq.suport.usuari.toString());
    }
    var grGestors = new GlideRecord('x_udg_q_gestorsuniversitat');
    grGestors.addQuery('actiu', true);
    grGestors.query();
    while (grGestors.next()) {
        if (notifica_a.indexOf(grGestors.usuari.toString()) == -1) {
			notifica_a.push(grGestors.usuari.toString());}	
    }
    current.notificat_a = notifica_a.join(',');

})(current, previous);]]></script>
